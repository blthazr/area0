{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Loose versioning for non-semver packages",
      "matchPackageNames": ["docker"],
      "matchPackagePatterns": [
        "plex",
        "qbittorrent"
      ],
      "versioning": "loose"
    }
  ]
}
